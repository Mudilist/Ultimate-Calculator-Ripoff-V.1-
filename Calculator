print(os.date())
Y = 0 ---Smaller number
X = 0 ---Bigger number
    
a = Y + X

print(a)print("addition")

------------------------

n = 0 ---Bigger number
m = 0 ---Smaller number

f = m - n

print(f)print("subtraction")

------------------------


e = 0 ---Bigger number
f = 0 ---Smaller number

r=e*f

print(r)print("multiplication")

------------------------

q = 0 ---Bigger number
i = 0 ---Smaller number

b=q/i

print(b)print("division")

-----------------------

l = 0 ---Principal
k = 0 ---Rate
j = 0 ---Time(years)

o = l*(1+k*j)

print(o)print("simple intrest")

-----------------------

P = 0 ---Principal
R = 0 ---Rate
T = 0 ---Time(years)

A = P*1+R*T^2

print(A)print("compound interest")

-----------------------

h = 0
r = 0
v = 3.14*r*r*h
print(v)print("Volume of the Cylinder")

-----------------------

Q = 0 --coefficient
X = 0 --exponent
B = Q*10^X
print(B)print("scientific notation")

